{
  "name": "gorgeous-ui",
  "version": "0.1.0",
  "license": "MIT",
  "private": false,
  "description": "A Component Library for Vue.js.",
  "main": "lib/gorgeous-ui.common.js",
  "bugs": {
    "url": "https://github.com/Mr-Alexx/gorgeous-ui/issues"
  },
  "scripts": {
    "demo": "vue-cli-service serve --mode demo",
    "doc": "vue-cli-service serve --mode doc",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:entry": "node build/build-entry.js",
    "test:unit": "vue-cli-service test:unit",
    "build:dev": "webpack --config build/build-dev.js",
    "build:theme": "gulp build --gulpfile build/gulpfile.js",
    "lib": "vue-cli-service build --target lib --name gorgeous-ui --dest lib src/index.js && npm run build:theme"
  },
  "dependencies": {
    "async-validator": "^1.10.0",
    "deepmerge": "^2.2.1",
    "normalize-wheel": "^1.0.1",
    "throttle-debounce": "^2.0.1"
  },
  "peerDependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-unit-mocha": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-standard": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "file-save": "^0.2.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "json-templater": "^1.2.0",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "uppercamelcase": "^3.0.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2"
  },
  "engines": {
    "node": ">= 10.12.0",
    "npm": ">= 6.4.1"
  }
}
